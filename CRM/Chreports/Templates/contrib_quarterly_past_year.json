{ 
    "name": "contrib_quarterly_past_year",
    "title": "Last Year inc. Today",
    "type": "summary",
    "template": "chreports/contrib_period_detailed",
    "period":"quarterly",
    "entity": "contribution",
    "fields":{
        "receive_date_start": {"default" : true}
    },
    "order_bys":{
        "receive_date_start": {
            "order": "ASC"
        }
    },
    "use_default_filters" : true,
    "groupByClause" : false,
    "filters":{
        "contribution_status_id": {
            "in": [
                "1"
            ]
        },
        "receive_date": {
            "relative": "ending.year"
        }
    }
}